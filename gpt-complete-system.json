  "oddsIntegration": {
    "description": "How to fetch and analyze live odds",
    "process": [
      "1. Fetch live odds using The Odds API for baseball_mlb",
      "2. Compare model predictions with bookmaker odds",
      "3. Calculate implied probability from American odds",
      "4. Find value bets where model probability > implied probability",
      "5. Apply Kelly Criterion for bet sizing recommendations"
    ],
    "oddsFormats": {
      "american": "Negative odds = favorite (e.g., -150), Positive = underdog (e.g., +130)",
      "impliedProbability": "For negative: |odds| / (|odds| + 100), For positive: 100 / (odds + 100)"
    },
    "valueBetting": {
      "definition": "Bet when model probability > implied probability from odds",
      "example": "Model: Yankees 60%, Odds: +110 (47.6% implied) = VALUE BET",
      "kellyCriterion": "(bp - q) / b where b=odds, p=model prob, q=1-p"
    }
